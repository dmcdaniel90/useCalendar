{
    "name": "use-fullcalendar",
    "version": "1.1.1",
    "description": "An easy to use calendar component for React using FullCalendar.js and TailwindCSS",
    "keywords": [
        "calendar",
        "react",
        "tailwindcss",
        "fullcalendar",
        "hooks",
        "typescript"
    ],
    "bugs": {
        "url": "https://github.com/dmcdaniel90/use-calendar/issues"
    },
    "author": "Devin McDaniel <dmcdaniel9@gmail.com>(https://code.devinmcdaniel.com)",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dmcdaniel90/useCalendar.git"
    },
    "files": [
        "dist"
    ],
    "main": "dist/index.js",
    "type": "module",
    "license": "MIT",
    "scripts": {
        "dev": "vitest",
        "build": "tsc",
        "ci": "npm run build && npm run check-format && npm run check-exports && npm run test",
        "format": "prettier --write .",
        "test": "vitest run",
        "check-format": "prettier --check .",
        "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
        "local-release": "changeset version && changeset publish",
        "prepublishOnly": "npm run ci"
    },
    "devDependencies": {
        "@arethetypeswrong/cli": "^0.18.2",
        "@changesets/cli": "^2.29.5",
        "@testing-library/react": "^16.3.0",
        "@types/jest": "^30.0.0",
        "@types/react": "^19.1.9",
        "@typescript-eslint/eslint-plugin": "^8.39.0",
        "@typescript-eslint/parser": "^8.39.0",
        "eslint": "^9.32.0",
        "prettier": "^3.6.2",
        "typescript": "^5.9.2",
        "vitest": "^3.2.4"
    },
    "dependencies": {
        "@fullcalendar/core": "^6.1.18",
        "@fullcalendar/daygrid": "^6.1.18",
        "@fullcalendar/react": "^6.1.18"
    },
    "peerDependencies": {
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "tailwindcss": "^3.4.14"
    },
    "engines": {
        "node": ">=22.0.0"
    }
}
